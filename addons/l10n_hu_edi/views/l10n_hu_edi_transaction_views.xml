<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="l10n_hu_edi_transaction_form" model="ir.ui.view">
        <field name="name">l10n_hu_edi.transaction.form</field>
        <field name="model">l10n_hu_edi.transaction</field>
        <field name="arch" type="xml">
            <form string="NAV Transaction">
                <sheet>
                    <group>
                        <field name="credentials_mode" readonly="1"/>
                        <field name="state" readonly="1"/>
                        <field name="invoice_id" readonly="1"/>
                        <field name="send_time" readonly="1"/>
                        <field name="transaction_code" readonly="1"/>
                        <field name="message_html" readonly="1"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>